        "build": "node src/build/scripts/production-build.js",
        "clean-build": "rimraf ./dist",
        "docker-build": "npm run clean-build && npm run build && docker build . -t foreldrepengesoknad",
        "docker-start": "docker run --name foreldrepengesoknad -p 8080:8080 -d foreldrepengesoknad",
        "docker-kill": "npm run docker-stop && npm run docker-rm",
        "docker-rm": "docker rm foreldrepengesoknad",
        "docker-stop": "docker stop foreldrepengesoknad",
        "docker-rmi": "npm run docker-kill && docker rmi foreldrepengesoknad",
        "lint": "eslint --quiet \"./src/**/*.{js,ts,tsx}\"",
        "lint:fix": "eslint --quiet --fix \"./src/**/*.{js,ts,tsx}\"",
        "tsc": "tsc  --pretty",
        "start-express": "node ./server.js",
        "dev-api": "node ./mock-api-server.js",
        "dev": "node ./src/build/scripts/start-dev-nodecorator.js",
        "start": "node ./src/build/scripts/start-dev.js",
        "jest": "jest --color --config ./jest/jest.config.js --maxWorkers 2",
        "jest-changed": "jest -o --color --config ./jest/jest.config.js --maxWorkers 2",
        "jest-coverage": "jest --config ./jest/jest.config.js --coverage --collectCoverageFrom=src/**/*.{ts,tsx} --collectCoverageFrom=!src/**/*.d.{ts,tsx}",
        "jest-watch": "jest --watch --verbose --config ./jest/jest.config.js",
        "snyk-protect": "snyk protect",
        "prepare": "npm run snyk-protect",
        "postinstall": "husky install",
        "storybook": "start-storybook -p 9001",
        "deploy-storybook": "build-storybook -o .static_storybook && storybook-to-ghpages --existing-output-dir=.static_storybook"
    },
