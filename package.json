    "scripts": {
        "build": "node src/build/scripts/production-build.js",
        "clean-build": "rimraf ./dist",
        "docker-build":
            "npm run clean-build && npm run build && docker build . -t foreldrepengesoknad",
        "docker-start":
            "docker run --name foreldrepengesoknad -p 8080:8080 -d foreldrepengesoknad",
        "docker-kill": "npm run docker-stop && npm run docker-rm",
        "docker-rm": "docker rm foreldrepengesoknad",
        "docker-stop": "docker stop foreldrepengesoknad",
        "docker-rmi": "npm run docker-kill && docker rmi foreldrepengesoknad",
        "lint": "tslint-config-prettier-check ./tslint.json",
        "start-express": "node ./server.js",
        "start-mock-api-server": "node ./mock-api-server.js",
        "start": "node ./src/build/scripts/start-dev.js",
        "start-no-decorator":
            "node ./src/build/scripts/start-dev-nodecorator.js",
        "precommit": "pretty-quick --staged",
        "cypress":
            "export PORT=8081 && npm run start-express & wait-on -v -t 15000 http://localhost:8081 && echo $(npm config get prefix)/bin/cypress run",
        "cy":
            "cypress open & npm run build && export PORT=8081 && npm run start-express",
        "jest": "jest --verbose --config ./jest/jest.config.js"
    },
