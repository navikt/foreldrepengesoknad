        "prettier": "prettier --write src",
        "start-express": "node ./server.cjs",
        "dev-api": "node --loader ts-node/esm ./mock-api/mock-api-server.ts",
        "dev": "node ./dev-server.cjs",
        "dev-vite": "vite",
        "preview": "vite preview",
        "build-storybook": "storybook build -o .storybook-static-build",
        "storybook": "storybook dev --quiet -p 7003",
        "compiler": "tsc --noEmit --watch"
    },
    "author": "NAV",
