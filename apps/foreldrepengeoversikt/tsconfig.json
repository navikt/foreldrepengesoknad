{
    "extends": "@navikt/fp-config-typescript/tsconfig.json",
    "compilerOptions": {
        "moduleResolution": "bundler",
        "baseUrl": "src",
        "jsx": "react-jsx",
        "allowImportingTsExtensions": true,
        "paths": {
            "storybookData/*": ["./../.storybook/storybook-data/*"]
        },
        "types": ["vite/client", "@testing-library/jest-dom", "vitest/globals"]
    },
    "include": ["src", "mock-api", "vitest", "../../@types/vite.env.d.ts"],
    "exclude": ["node_modules"]
}
