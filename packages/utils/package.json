{
    "name": "@navikt/fp-utils",
    "main": "index.ts",
    "type": "module",
    "types": "index.ts",
    "scripts": {
        "test": "vitest --project=jsdom --watch=false",
        "test:watch": "vitest --project=jsdom --watch=true",
        "test:coverage": "vitest --project=jsdom --coverage",
        "test:browser": "vitest --project=browser",
        "test:browser:watch": "vitest --project=browser --watch=true",
        "lint:eslint": "eslint \"./src/**/*.{js,ts,tsx}\"",
        "lint:tsc": "tsc --noEmit",
        "lint:fix": "eslint --fix './src/**/*.{js,ts,tsx}' --color",
        "prettier": "prettier --write src"
    },
    "dependencies": {
        "@navikt/fp-constants": "workspace:*",
        "@navikt/fp-types": "workspace:*",
        "dayjs": "1.11.19",
        "detect-browser": "5.3.0",
        "i18n-iso-countries": "7.14.0",
        "react": "19.2.3",
        "react-intl": "8.0.6"
    },
    "devDependencies": {
        "@formatjs/cli-lib": "8.0.4",
        "@navikt/fp-config-eslint": "workspace:*",
        "@navikt/fp-config-typescript": "workspace:*",
        "@navikt/fp-config-vite": "workspace:*",
        "@testing-library/jest-dom": "6.9.1",
        "@types/react": "19.2.7",
        "eslint": "9.39.2",
        "fast-glob": "3.3.3",
        "playwright": "1.57.0",
        "typescript": "5.9.3",
        "vite": "7.3.0",
        "vitest": "4.0.16"
    },
    "packageManager": "pnpm@10.18.3"
