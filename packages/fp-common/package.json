        "intl": "^1.2.5",
        "react": "^18.2.0",
        "react-intl": "^6.4.4"
    },
    "files": [
        "package.json",
        "/lib"
    ],
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
