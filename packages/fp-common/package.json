        "@types/node": "^20.5.0",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "classnames": "^2.3.2",
        "date-fns": "^2.30.0",
        "dayjs": "^1.11.9",
        "formik": "^2.2.9",
        "history": "^5.3.0",
        "react": "^18.2.0",
        "react-aria-menubutton": "^7.0.3",
        "react-collapse": "^5.1.1",
